# Укажите название переменной с активным токеном (пример: TOKEN_TURKEY)
ACTIVE_TOKEN=TOKEN_TURKEY

# Telegram Bot Tokens по странам (можно переопределять)
TOKEN_TURKEY=111111111:AAExampleTurkeyToken
TOKEN_INDONESIA=222222222:AAExampleIndonesiaToken
TOKEN_ARABIC=333333333:AAExampleArabicToken
TOKEN_VIETNAM=444444444:AAExampleVietnamToken
TOKEN_BRAZIL=555555555:AAExampleBrazilToken

# OpenAI API Key (начинается с sk- или sk-proj-)
OPENAI_API_KEY=sk-proj-xxx-exampleOpenAiKey

# Количество бесплатных сообщений до предложения оплаты
FREE_MESSAGES=10
